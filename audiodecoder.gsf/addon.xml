<?xml version="1.0" encoding="UTF-8"?>
<addon id="audiodecoder.gsf" version="2.0.3.1" name="GSF Audio Decoder" provider-name="spiff">
  <requires>
    <import addon="kodi.binary.global.main" version="1.0.14"/>
    <import addon="kodi.binary.global.filesystem" version="1.0.2"/>
    <import addon="kodi.binary.instance.audiodecoder" version="2.0.0"/>
  </requires>
  <extension point="kodi.audiodecoder" name="gsf" extension=".minigsf" tags="true" library_linux="audiodecoder.gsf.so.2.0.3"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">VIOGSF based Audio Decoder</summary>
    <description lang="en">GSF is a addon for Kodi which adds the ability to play Game Boy Advance music files, the GSF format (MINIGSF and GSFLIB files). This player is based on the Visual Boy Advanced emulator, although the sound quality appears to be identical to Highly Advanced.</description>
    <platform/>
    <license>GPL-2.0</license>
    <source>https://github.com/xbmc/audiodecoder.gsf</source>
    <assets>
      <icon>icon.png</icon>
    </assets>
  </extension>
</addon>
